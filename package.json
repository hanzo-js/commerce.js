{
  "name": "commerce.js",
  "version": "7.4.0",
  "packageManager": "pnpm@9.15.4",
  "description": "Hanzo Commerce API client â€” billing, subscriptions, payments, usage tracking. Vanilla JS, no framework deps.",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./billing": {
      "import": "./dist/billing.mjs",
      "require": "./dist/billing.js",
      "types": "./dist/billing.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/types.ts src/billing.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/types.ts src/billing.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanzoai/commerce.js.git"
  },
  "keywords": [
    "commerce",
    "billing",
    "subscriptions",
    "payments",
    "usage",
    "hanzo",
    "api-client",
    "vanilla-js"
  ],
  "author": "Hanzo AI, Inc. <hi@hanzo.ai>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/hanzoai/commerce.js/issues"
  },
  "homepage": "https://github.com/hanzoai/commerce.js#readme",
  "dependencies": {
    "@hanzo/commerce": ">=7.4.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
